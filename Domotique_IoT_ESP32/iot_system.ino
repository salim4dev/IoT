//fonction qui renvoi l'etat du bouton
String outputState(int output){
  if(digitalRead(output)){
    return "checked";
  }
  else {
    return "";
  }
}


// fonction qui remplace les variables (placeholder) par ce qui convient 
String processor(const String& var){
 
if(var == "STATE2"){
    return outputState(2);}
 else if(var == "STATE4"){
    return outputState(4);}
 else if(var == "STATE33"){
    return outputState(33);}
 else if(var == "STATE25"){
    return outputState(25);}
     else if(var == "STATE19"){
    return outputState(19);}
    
else if(var == "TEMPERATURE"){
    return readDHTTemperature();
  }
  else if(var == "HUMIDITY"){
    return readDHTHumidity();
  }

else if(var == "GAZSTATE"){
    if(digitalRead (gaspin)){
      gazstate = "pas de fuite";
    }
    else{
      gazstate = "fuite gaz!!";
    }
    Serial.print(gazstate);
    return gazstate;
  }
  
  return String();
}









